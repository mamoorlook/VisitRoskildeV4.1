            </StackPanel.DataContext>
            <StackPanel Orientation="Horizontal" Height="130">
                <TextBlock x:Name="tempTextBlock"  TextWrapping="Wrap"  Text="{Binding Weather.Temperature}" FontSize="140" Width="131" TextAlignment="Right" Foreground="#FF2E2E2E"/>
                <TextBlock TextWrapping="Wrap" Text="°C" FontSize="100" Margin="0,0,309,0" Foreground="#FF2E2E2E"/>
            </StackPanel>
            <TextBlock  TextWrapping="Wrap"  Text="{Binding Weather.City}" FontSize="30" Foreground="#FF2E2E2E" Margin="65,0,117,0"/>

